{
	"easycom": {
		// npm安装的方式不需要前面的"@/"，下载安装的方式需要"@/"
		// npm安装方式
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
		// 下载安装方式
		// "^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "点单"
			}
		}, 
		{
			"path": "pages/tongji/tongji",
			"style": {
				"navigationBarTitleText": "统计",
				"enablePullDownRefresh": false
			}

		}, 
		{
			"path": "pages/wode/wode",
			"style": {
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": false
			}

		}, 
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "登录",
				"enablePullDownRefresh": false
			}

		}
	    ,{
            "path" : "pages/contact/contact",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "关于我们",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/xiadan/xiadan",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "下单",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/address/address",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "地址管理",
                "enablePullDownRefresh": false
            }
            
        },{
            "path" : "pages/shoucang/shoucang",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "收藏",
                "enablePullDownRefresh": false
            }
            
        },{
            "path" : "pages/register/register",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "注册",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/changePasswd/changePasswd",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "修改密码",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#Ffcc00",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"selectedColor": "red",
		"list": [{
				"iconPath": "static/dianDanInActive.png",
				"selectedIconPath": "static/dianDanAction.png",
				"text": "点单",
				"pagePath": "pages/index/index"
			},
			{
				"iconPath": "static/tongJiInAction.png",
				"selectedIconPath": "static/tongJiAction.png",
				"text": "统计",
				"pagePath": "pages/tongji/tongji"
			},
			{
				"iconPath": "static/woDeInAction.png",
				"selectedIconPath": "static/woDeAction.png",
				"text": "我的",
				"pagePath": "pages/wode/wode"
			}
		]
	}
}